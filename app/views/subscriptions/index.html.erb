<table>
    <tr>
      <th>Name</th>
      <th>Course Description</th>
      <th>Grade</th>
    </tr>

<tbody>
    <% @subscriptions.each do |subscription| %>
      <tr>
        <td><%= subscription.course.name %></td>
        <td><%= subscription.course.description %></td>
        <td><%= subscription.grade %></td>
        <td><%= link_to 'Unenroll', subscription_path(subscription), method: :delete, class: 'btn btn-small'%>
        </td>
      </tr>
    <% end %>
  </tbody>

</table>