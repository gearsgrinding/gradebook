
<h2>All Users</h2>

<table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
    </tr>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
      </tr>
    <% end %>
  </tbody>
</table>